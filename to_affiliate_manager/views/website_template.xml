<?xml version="1.0" encoding="UTF-8"?><openerp>	<data>		<template id="to_affiliate_register_menu" inherit_id="website.layout"			name="Affiliate Register">			<xpath				expr="//body//ul[@class='dropdown-menu js_usermenu']"				position="inside">				<li t-if="not user_id.partner_id.to_is_affiliate">					<a t-attf-href="/affiliate" role="menuitem">Join Affiliate</a>				</li>				<li t-if="user_id.partner_id.to_is_affiliate">					<a t-attf-href="/affiliate/#{ user_id.id }" role="menuitem">Affiliate Code					</a>				</li>			</xpath>		</template>		<template id="to_affiliate" name="Affiliate Register">			<t t-call="website.layout">				<div id="wrap">					<div class="oe_structure">						<section data-snippet-id="text-block" class="mb16">							<div class="container">								<div class="row" t-if="not user_id.partner_id.to_is_affiliate">									<div class="col-md-12 text-center mb16" data-snippet-id="colmd">										<h2>Join Our Affiliate Program</h2>									</div>									<div class="col-md-12 mt16 mb16">										<form action="/affiliate/join" method="post"											class="form-horizontal mt32" enctype="multipart/form-data">											<input type="hidden" name="user" t-att-value="user_id.id" />											<input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>											<div class="form-group" t-if="len(kwargs['to_res_company']) > 1">												<label for="company" class="col-md-3 col-sm-4 control-label">Preferred Seller</label>												<div class="col-md-7 col-sm-8">													<select name="to_res_company" class="form-control"														id="company">														<option value="0">Select a Company</option>														<t t-foreach="kwargs['to_res_company']" t-as="company">															<t t-if="company.aff_allowed">																<option t-att-value="company.id">																	<span t-esc="company.name" />																</option>															</t>														</t>													</select>												</div>											</div>											<div class="form-group">												<div class="col-md-offset-3 col-sm-offset-4 col-sm-8 col-md-7">													<button class="btn btn-primary btn-lg">Join</button>												</div>											</div>										</form>									</div>								</div>								<div class="row" t-if="user_id.partner_id.to_is_affiliate">									<div class="col-md-12 text-center mb16" data-snippet-id="colmd">										<h2>Welcome to Affiliate Program</h2>									</div>									<div class="col-md-12 mb16 mt16" t-if="kwargs['aff_code']">										<p>											In order to get started with the code											<code>												<span t-esc="kwargs['aff_code'].name" />											</code>											,											just copy the URL											<code>												<span t-esc="kwargs['aff_code'].url" />											</code>											and share it with your friends and partners.											You can put the											URL on your social network like Facebook,											LinkedIn											or put it on											webpages where you can to get more and more											customers.										</p>									</div>								</div>							</div>						</section>					</div>				</div>			</t>		</template>		<template id="to_affiliate_ad" name="Affiliate Advertisement">			<t t-call="website.layout">				<div id="wrap">					<div class="oe_structure">						<section data-snippet-id="text-block" class="mb16">							<div class="container">								<div class="row">									<h2>Chose a Advertisement html code, and copy it on your website</h2>								</div>								<div class="row">									<div class="col-md-12 mt16 mb16">										<form action="/affiliate/join" method="post"											class="form-horizontal mt32" enctype="multipart/form-data">											<input type="hidden" name="user" t-att-value="user_id.id" />											<input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>											<div class="form-group" t-if="len(kwargs['to_res_company']) > 1">												<label for="company" class="col-md-3 col-sm-4 control-label">Preferred Seller</label>												<div class="col-md-7 col-sm-8">													<select name="to_res_company" class="form-control"														id="company">														<option value="0">Select a Company</option>														<t t-foreach="kwargs['to_res_company']" t-as="company">															<t t-if="company.aff_allowed">																<option t-att-value="company.id">																	<span t-esc="company.name" />																</option>															</t>														</t>													</select>												</div>											</div>										</form>									</div>								</div>							</div>						</section>					</div>				</div>			</t>		</template>					</data></openerp>